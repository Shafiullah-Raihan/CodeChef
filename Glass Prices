#include <bits/stdc++.h>
using namespace std;
#define bismillah     ios_base::sync_with_stdio(false);  cin.tie(0);


int main() {
    bismillah;

    int a, b;
    cin>>a>>b;
    if(a*2>= b){
        cout<< "METAL"<<endl;
    }
    else{
        cout<<"PLASTIC"<<endl;
    }

    return 0;
}
