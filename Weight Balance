///https://www.codechef.com/practice/course/1to2stars/LP1TO201/problems/WEIGHTBL?tab=statement

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int x,y,a,b,m,r;
	    cin>>x >>y >>a >>b >>m;
	    r=y-x;
	    if(r<a*m|| r>m*b)
	    {
	        cout<<"0"<<endl;
	    }
	    else
	    {
	        cout<<"1"<<endl;
	    }
	}
	return 0;
}
